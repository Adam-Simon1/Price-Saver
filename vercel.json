{
  "version": 2,
  "builds": [
    {
      "src": "src/login.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/login.js"
    }
  ],
  "crons":[
    {
      "path": "/src/public/js/script_kaufland.js",
      "schedule": "0 * * * *"
    },
    {
      "path": "/src/public/js/script_tesco.js",
      "schedule": "0 * * * *"
    }
  ]
}